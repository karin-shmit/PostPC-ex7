<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="postpc.huji.SandwichApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ex7">
        <activity android:name="postpc.huji.SandwichApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewOrderActivity" />
        <activity android:name=".EditOrderActivity" />
        <activity android:name=".OrderInProgressActivity" />
        <activity android:name=".OrderIsReadyActivity" />
    </application>

</manifest>